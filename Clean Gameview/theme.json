{
    "name": "Clean Gameview",
    "author": "SuchMeme",
    "target": "Library",
    "version": "v1.1",
    "manifest_version": 2,
    "description": "Makes the game view less cluttered, and shows more of the game's art",
    "inject": {
        "shared.css": ["SP"]
    },
    "patches": {
        "Show Steam Cloud": {
            "type": "checkbox",
            "default": "No",
            "values": {
                "Yes": {
                    "steamcloud.css": ["SP"]
                },
                "No": {}
            }
        }
    }
}